    "esbuild": "~0.23.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.5.0",
    "puppeteer-core": "^22.12.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  }
}
